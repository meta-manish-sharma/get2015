<?xml version="1.0" encoding="UTF-8"?>
<web-app>
<servlet>
		<servlet-name>Validate</servlet-name>
        <servlet-class>com.servlets.Validate</servlet-class>
</servlet>
<servlet>
		<servlet-name>ShowResult</servlet-name>
        <servlet-class>com.servlets.ShowResult</servlet-class>
</servlet>
<servlet-mapping>
        <servlet-name>Validate</servlet-name>
        <url-pattern>*.Validate</url-pattern>
</servlet-mapping>
<servlet-mapping>
        <servlet-name>ShowResult</servlet-name>
        <url-pattern>*.ShowResult</url-pattern>
</servlet-mapping>
<welcome-file-list>
	<welcome-file>index.html</welcome-file>
</welcome-file-list>
</web-app>